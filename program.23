#include <iostream>
using namespace std;
int main() {
    float r;
    cout << "Enter the radius of the circle: ";
    cin >> r;
    float area = pi * r * r;
    float circumference = 2 * pi * r;
    cout << "Area of the circle: " << area << endl;
    cout << "Circumference of the circle: " << circumference << endl;
    return 0;
}
