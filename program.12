#include <iostream>
using namespace std;
int main() {
    const float pi = 3.14159;
    float radius, height, volume;
    cout << "Enter the radius of the cylinder: ";
    cin >> radius;
    cout << "Enter the height of the cylinder: ";
    cin >> height;
    volume = pi * radius * radius * height;
    cout << "The volume of the cylinder is: " << volume << endl;
    return 0;
}